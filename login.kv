    		source: 'new.jpg'
    		size: self.size
    		pos: self.pos
    		
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'

        Image:
            source: 'iitlogo.png'
            size_hint: None, None
            size: 300, 300			
    
     
	# size_hint defines the widget width (0-1) representing
	# a percentage of 100% and height of the window

	    
    GridLayout:
    	rows: 4
    	spacing:20
    	
    	FloatLayout:
	    	
	    #	Label:
	    	#	font_size: 60
	    	#	text: "Login As"
	    		#color: 0, 0, 0, 1
	    	#	pos_hint: {"x": -0.1, "y": -0.445}
	    		  	
	 	          	
			Button:    
	            text: "Student"
	            font_size: 50
	    	#	background_color: 0.75, .75, 0.75, .7
	            size: 150, 40          #size of button background
	            size_hint: .2, .08
	            pos_hint: {"x": 0.3, "y": 0.02}
	           # pos: 150,200            #button position
	            on_release: root.login_as("student")
	            
	        Button:    
	            text: "Admin"
	            font_size: 50
