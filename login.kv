<LoginPage>:
    name: "login_page"
    
    canvas:
    	Rectangle:
    		source: '1.jpg'
    		size: self.size
    		pos: self.pos
    		
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'top'

        Image:
            source: 'iitlogo.png'
            size_hint: None, None
            size: 300, 300		
    
     
	# size_hint defines the widget width (0-1) representing
	# a percentage of 100% and height of the window

	    
    GridLayout:
    	rows: 4
    	spacing:20
    	
    	
    		
    	
    	FloatLayout: 	
    	#name text    		
    			  
	    #name block to write  		
        	TextInput:
            	id: login                     #this login contain the enter text
            	hint_text: "user name"
            	size_hint: .2, .05
            	font_size: 24
	    		pos_hint: {"x": .4, "y": .55}    
	    		mutliline: False     	
            	
		
					
        	TextInput:
        		id: pass
            	password: True 		# hide password
            	hint_text: "****"
            	font_size: 24
            	size_hint: .2, .05
	    		pos_hint: {"x": .4, "y": .45}    
	    		mutliline: False 
	    		
	        Label:
	            text: 'Remember my login'
				font_size: 22
	    		#color: 0, 0, 0, 1
	    		size: 50, 40
	    		background_color: .88, .88, .88, 1
	    		size_hint: .2, .1
	    		pos_hint: {"x": .39, "y": .35}	
	    		
	    	CheckBox:
	    		id: checkbox              #checkbox.active
	    		size_hint: .12, .12
	    		#size: 200, 200
	    		color: 0, 0, 0, 1
	    		background_color: .88, .88, .88, 1
	    		pos_hint: {"x": .345, "y": .34}	
	    	   # on_active: checkbox.text: 'true'				
            	
            	
			Button:    
	            text: "Log in"
	            #color: 0, 1, 0, .8
	          #  background_color: 0, 1, 0, .75	    
	            background_color: 0.62, 1, .2, 1        
	            font_size: 30
	            size: 100, 50
	            size_hint: .2, .05
	            pos_hint: {"x": .4, "y": 0.3}
	            on_release: root.verify_credentials()
	            
	            
	        Button:    
	            text: "Home"
	            font_size: 30
	            size: 40, 40
	            background_color: 0.21, 0.58, 1, .7            
	            size_hint: .08, .05
	            pos_hint: {"x": .82, "y": .88}
	            
                on_release: app.root.current = "Welcome_page"
	            
	            
	            
